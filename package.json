{
  "name": "i32",
  "version": "0.0.0-development",
  "description": "32-bit signed integer math",
  "keywords": [
    "i32",
    "int",
    "int32",
    "integer",
    "math"
  ],
  "author": "Daniel Cassidy <mail@danielcassidy.me.uk>",
  "homepage": "https://github.com/softwareventures/i32",
  "bugs": "https://github.com/softwareventures/i32",
  "repository": "github:softwareventures/i32",
  "license": "ISC",
  "scripts": {
    "pre-commit": "precise-commits",
    "fix": "tsc --noEmit && eslint . --fix && prettier --write .",
    "lint": "tsc --noEmit && eslint . && prettier --check .",
    "prepare": "husky install && tsc",
    "semantic-release": "semantic-release",
    "test": "ava"
  },
  "sideEffects": false,
  "engines": {
    "node": "^12 || ^14 || >=16"
  },
  "dependencies": {
    "@softwareventures/array": "^0.31.0 || ^0.33.0 || ^2.0.0 || ^3.0.0 || ^4.0.0 || ^5.0.0",
    "@types/imul": "^1.0.0",
    "imul": "^1.0.1",
    "tslib": "2.4.0"
  },
  "devDependencies": {
    "@softwareventures/eslint-config": "5.1.5",
    "@softwareventures/precise-commits": "2.0.20",
    "@softwareventures/prettier-config": "3.0.0",
    "@softwareventures/semantic-release-config": "2.0.2",
    "@softwareventures/tsconfig": "6.0.0",
    "ava": "4.2.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.13.0",
    "husky": "7.0.4",
    "prettier": "2.6.2",
    "semantic-release": "17.4.7",
    "ts-node": "10.7.0",
    "typescript": "4.6.3"
  },
  "eslintConfig": {
    "root": true,
    "extends": "@softwareventures"
  },
  "prettier": "@softwareventures/prettier-config",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "release": {
    "extends": "@softwareventures/semantic-release-config"
  }
}
