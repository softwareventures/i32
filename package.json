    "ava": "3.15.0",
    "cz-conventional-changelog": "3.3.0",
    "semantic-release": "17.4.4",
    "ts-node": "9.1.1",
    "tslint": "6.1.3",
    "typescript": "4.3.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
